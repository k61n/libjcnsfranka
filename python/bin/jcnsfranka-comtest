#!/usr/bin/env python3

import sys

from pyjcnsfranka.robot import comtest


limit_rate = (sys.argv[1].lower() == 'true' or sys.argv[1] == '1') \
    if len(sys.argv) > 1 else True
cutoff_frequency = float(sys.argv[2]) if len(sys.argv) > 2 else 1000
if cutoff_frequency in [100.0, 1000.0]:
    comtest('192.168.201.2', limit_rate, cutoff_frequency)
